<table class="table table-striped">
  <caption>Posts in your blog so far</caption>
  <thead>
  <tr>
    <th>Title</th>
    <th>Created At</th>
    <th>Updated At</th>
    <th></th>
  </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
  <tr>
    <td><%=	post.title %></td>
    <td><%=	post.created_at %></td>
    <td><%=	post.updated_at %></td>
    <td><%= link_to "Edit", edit_post_path(post), class: "btn" %></td>
  </tr>
  <% end %>
  </tbody>
</table>